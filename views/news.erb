	<!doctype html>
	<html lang="en">
	<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	
	<!-- Google Fonts -->
	<!-- Swap out "Roboto" for the font(s) you want to use -->
	<link href="https://fonts.googleapis.com/css?family=Calibri&display=swap" rel="stylesheet">
	
	<!-- Font Awesome -->
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.12.0/css/all.css" crossorigin="anonymous">
	
	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
	
	<!-- Your own CSS -->
	<link rel="stylesheet" href="/styles.css">
    
    
	<title>News & Weather</title>
	</head>
	<body>
	
	<div class="jumbotron">
	    <h1  class="display-5">Your Customized Dashboard</h1>
         <p>Serving your needs 24/7</p>
	</div>
	
	<div class="container mt-5">

	<div class = "row-sm">
		</div>
	        <h5> Currently it is <%= @tempnow %> degrees and <%= @conditions %> in <%= @location %>.</h5>
	<div class = "row-sm">
    <h6> Next Week's Forecast </h6>
    
    </div>
    <div class="row"><ul> <% for temp in @temp %> <div class="col-sm"> <%= temp %>
    </div> <% end %> </ul>

	<ul> <% for condition in @condition %>
	<div class="col-sm"> <%= condition %>
	</div> <% end %> </ul> </div>
	
	<div class = "row-sm">
	<h3> News Headlines </h3>
    </div>
    

    <% for link in @headline %>
	<% for name in @titles %>
	<div class= "row-sm">
	<div class="card" style="width: 110rem;">
	<div class="card-body">
	<h5 class="card-title"><%= name %></h5>
	<a href=<%= link %> class="card-link">Full Story</a>
	</div> </div> </div>
	<% end %> <% end %>
	
    
	</div>
	
	<!-- Optional JavaScript -->
	<!-- jQuery first, then Popper.js, then Bootstrap JS -->
	<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
	<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
	</body>
	</html>